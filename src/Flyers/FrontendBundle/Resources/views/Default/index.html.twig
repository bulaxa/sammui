{% extends "angular.html.twig" %}

{% block stylesheets %}
    {{ parent() }}

    <link rel="stylesheet" href="{{ asset('bundles/frontend/css/app.css') }}"/>

{% endblock %}

{% block javascripts %}
    {{ parent() }}

    {#Crypto JS#}
    <script type="text/javascript"
            src="{{ asset("bundles/frontend/components/cryptojslib/rollups/sha1.js") }}"></script>
    <script type="text/javascript"
            src="{{ asset("bundles/frontend/components/cryptojslib/rollups/sha512.js") }}"></script>
    <script type="text/javascript" src="{{ asset("bundles/frontend/components/cryptojslib/rollups/md5.js") }}"></script>
    <script type="text/javascript"
            src="{{ asset("bundles/frontend/components/cryptojslib/components/enc-base64-min.js") }}"></script>

    {#Angular APP#}
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/app.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/services.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/translate/js/translate-app.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/translate/js/translate-services.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/translate/js/translate-controllers.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/controllers.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/filters.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/frontend/js/directives.js') }}"></script>

{% endblock %}